.clear{  
  clear: both;  
}  
.tab-bar{  
  position:fixed;
  border-top: 1px solid rgb(221, 220, 220);
  width:100%;
  bottom:0;
  left:0;
  background:#fff;
  font-size:24rpx;
  display: flex;
}  
 
