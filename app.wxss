  width: 40rpx;  
  height:40rpx;  
  margin-bottom:9rpx;
}  
.clear{  
  clear: both;  
}  
.tab-bar{  
  position:fixed;
  border-top: 1px solid rgb(221, 220, 220);
  width:100%;
  bottom:0;
  left:0;
  background:#fff;
  font-size:24rpx;
  display: flex;
}  
 
.button{
  margin: 130px;
}

/* TOP STAT */

.custom{
  position: fixed;
  width: 100%;
  top: 0;
  left: 0;
  height: 45px;
  background: #2C5CFF;
  z-index: 999;
}
.custom text{
  color: #fff;
  font-size: 34rpx;
  font-weight: 500;
  max-width: 280rpx;
}
.empty_custom{
  height: 45px;
  width: 100%;
}
.footer-height{
  width: 100%;
  height: 125rpx;
}

