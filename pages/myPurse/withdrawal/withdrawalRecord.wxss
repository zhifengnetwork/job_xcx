    font-size: 26rpx;
    color: #151515;
}
.-detail-cont{
    padding: 20rpx 0;
    border-bottom: 2rpx solid rgb(224, 224, 224);
    /* margin-bottom: 18rpx; */
}
.-d-list{
    display: flex;
    justify-content: space-between;
