/**/
.search-1-wrap{
   position:relative;
   display: flex;
   padding-top: 10rpx;
   border-bottom: 2rpx solid #fff;
}
.g-header-{
   position:fixed;
   top: 0;
   z-index: 10;
   width: 100%;
   padding: 10rpx 0; 
   background:#fff;

}
.-header-cont{
   display: flex;
   align-items:center;
}

.search-cont{
  padding: 10rpx 0 0;
  flex: 0 0 1;
  width:581rpx;
  margin: 0 60rpx 0 24rpx; 
  box-sizing: border-box;
}
.search{
   width:100%;
   border:1px solid #fa5fb7;
   border-radius: 36px;
   height:60rpx;
   padding-left:45rpx;
   box-sizing: border-box;
   display: inline-block;
   font-size: 24rpx;
   
}
.search-icon{
  width:21rpx;
  height:21rpx;
  position:absolute;
  left:45rpx;
  top:42rpx;

}
.search-r-text{
  display: block;
  font-size:24rpx;
  line-height:40rpx;
  color:#151515;
  font-weight:700;
  flex: 0 0 1;
}
/* subtitle start */
.subtitle{
  color:#151515;
  width: 581rpx;
  margin:0 60rpx 0 24rpx;
  line-height: 55rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
}

.subtitle text {
  text-align:center;
  font-size:28rpx;
  font-weight:700;
}
.c-body{
  padding-top:175rpx;
}
 /* LIST STYLE START */
 .c-title{
  width:100%;
  margin:20rpx 0 30rpx 0;
  display:flex;

}
.c-flame{
   width:26rpx;
   height:26rpx;
   margin-right:14rpx;
   margin-top:6rpx;
   vertical-align: top;
}
.c-title text{
  font-size:28rpx;
  font-weight: 700;
}
.c-l-info {
  margin-bottom:10rpx;
  border-radius:5px;
  background:#fff;
  align-items:center;
  display:flex;
  padding:25rpx 10rpx 25rpx;
}

.c-l-info .c-logo{
  width:90rpx;
  height:90rpx;
  flex:0 0 1;
  border-radius: 50%;
}
.c-info-text{
  margin-left:21rpx;
}
.c-info-title{
  font-weight: 700;
  font-size: 28rpx;
  
}
.c-info-msg{
  margin-top: 18rpx;
}
.c-info-msg text{
  font-size:24rpx;
  color:#5d5d5d;
}
.c-info-msg .c-msg{
  padding-right: 30rpx;
}
.c-msg3{
 font-size: 24rpx;
 color: #5d5d5d;
 margin-top:14rpx;


}
/* company info start */
.company-info{
  /* flex: 0 0 1; */
  margin-left: auto;
  text-align: right;
  font-size:28rpx;
}

.company-status{
   width: 100rpx;
   text-align: center;
   font-size: 24rpx !important;
   background:#ff54b5;
   padding: 6rpx 3rpx !important;
   border-radius: 2px;
   color:#fff;
   margin-top:18rpx;
}

.more-recommond{
   text-align: center;
   margin: 11rpx 0;
   color: #5d5d5d;
   font-size: 20rpx;
}

/* Latest job search start */
.search-job-list{
  width: 100%;
}
.s-portrait{
  width: 100%;
  overflow: hidden;
  margin-top: 18rpx;
  color:#5d5d5d;
  font-size: 24rpx;
}
.s-portrait .portrait-icon{
  width: 45rpx;
  height: 45rpx;
  vertical-align: middle;
  margin-right: 18rpx;
  border-radius: 50%;
}
.c-info-msg .company-status{
  margin-top:0;
}

