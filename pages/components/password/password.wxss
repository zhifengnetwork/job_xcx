.code-wrap{
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  z-index: 100;
  text-align: center;
}

.code-inner{
  width: 100%;
  height: 100%;
  display:block;
  background: rgba(0,0,0,0.5);
}

.invitation-txt{
  margin:80rpx 0 20rpx;
}
   
.invitation-code{
  /* width:422rpx; */
  position: absolute;
  top: 20%;
  left: 5%;
  right: 5%;
  width: 90%;
  background: #fff;
  padding: 40rpx;
  box-sizing: border-box;
  border-radius: 8rpx;
}
  
.pasw-msg{
    display:flex;
    position:relative;
    width:100%;
    align-items:flex-end;
    height:80rpx;
    line-height:80rpx;
    padding: 20rpx 0;
}

.key-info{
    background:red;
    width: 100%;
    height:80rpx;
    position:absolute;
    left:0;
    z-index:10000;
    opacity:0;
    bottom:0;
  }

.pasw-list{
    margin-right:30rpx;
    width:80rpx;
    font-size:40rpx;
    border-bottom:4rpx solid #151515;
}

.faget-password{
  margin-top:40rpx;
  text-align:right;
  font-size:26rpx;
  text-decoration:undeline;
  color:#666;
  text-decoration: underline;
}
        