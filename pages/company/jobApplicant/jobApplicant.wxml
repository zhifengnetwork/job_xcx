
  <!-- HEADER START -->
  <view class='g-header-'>
    <view class='-header-cont'>
      <navigator url='../../search/pSearch' class='search-cont s-nosearch-wrap'>
        <view class='search search-nosearch company-bColor-1'>输入所要查找的内容</view>
        <image class="search-icon" src='../../../static/images/public/search_icon.png'></image>
      </navigator>
    </view>
    <view class='subtitle search-subtitle'>
        <picker value="{{cat_id}}" range="{{jobArray}}" bindchange="jobChange" range-key="cat_name" range-value="cat_id">
          <view class="picker subtitle-msg">
            {{jobArray[jobIndex].cat_name}} 
            <icon class="iconfont icon-zhcc_xiangxiajiantou"></icon>
          </view>
        </picker>
        <view class="newsite_button m-left-auto" bindtap="selectDistrict">
          <view class="newsite_view" wx:if="{{showTST}}">选择地点</view>
          <view class="newsite_city">{{areaInfo}} <icon class="iconfont icon-zhcc_xiangxiajiantou"></icon></view>
          
        </view>

        <!-- 所有城市三级联动列表 -->
        <include src="../../template/address"/> 
        <!-- 地址选择接受 -->

    </view>
  </view>
  <view class='header-heigh'></view>
  <!-- HEADER END -->
  <view class='inner'>
      <block wx:for="{{hiringData}}"  wx:key="index">
        <navigator url="../personalDetail/personalDetail?id={{item.id}}">
          <view class='c-l-info'>
          <!-- <view class='c-l-info' bindtap='jumpDetails' data-id='{{item.id}}'> -->
            <image src='{{item.avatar}}' class="c-logo" />
            <view class='global-info-text'>
              <view class="global-search-cont">
                  <text class='c-info-title'>{{item.name}}</text>
                  <text class='c-salary'>{{item.salary}} /月</text>
              </view>
              <view class='global-search-cont c-info-msg'>
                  <view >
                      <text class='c-msg2'>{{item.work_age}}年</text>
                      <text >{{item.school_type}}</text>
                  </view>
                  
                  <view class="global-status-sty">
                      <view class='company-status company-bg-' wx:if="{{item.cat_name!=null}}">{{item.cat_name}}</view>
                      <view class='company-status company-bg-'>{{item.images==1 ? "有证书":"无证书"}}</view>
                  </view>
                  
              </view>
              <view class='global-search-cont c-info-msg'>
                  <text> </text>
                  <text>人才</text>
              </view>
            </view>
          </view>
        </navigator>
      </block>
  </view>
  <!-- FOOTER TARBAR START -->
  <view class='footer-height'></view>
  <import src="../../template/tabbar.wxml" />
  <template is="tabBar" data="{{tabBar}}" />
