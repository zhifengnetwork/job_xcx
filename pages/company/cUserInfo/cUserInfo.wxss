@import "../../template/common";
@import "../../template/font";
#wrap{
  width:100%;
  height:100%;
  background:#f8f8f8;
  font-size:28rpx;
}
.inner{
  margin: 0 24rpx;
}
.title{
  height:90rpx;
  line-height:90rpx;
  text-align: center;
  font-size:30rpx;
  color:#151515;
  font-weight:bold;
}
.c-l-info {
  background:none;
}
.c-l-info .c-logo{
  width:164rpx;
  height:140rpx;  
  border-radius: 4rpx;
}
.company-status {
  float: right;
}
.edit-info{
  width: 320rpx;
  border-radius:45rpx;
  height:88rpx;
  color:#fff;
  text-align: center;
  line-height:88rpx;
  margin:26rpx 0 47rpx;
  font-size:30rpx;
}
.c-info-text{
  margin-left:21rpx;
  width: 70%;
}
/* USER OPTION START */
.u-o-list{
  display: flex;
  justify-content: space-between;
  align-items:center;
  font-size: 30rpx;
  color:#656565;
  padding:6px 2px
}
.u-o-icon1{
  width:25rpx;
  height: 25rpx;
}
.u-o-icon2{
  width: 12rpx;
  height: 24rpx;
}
.u-o-text{
  font-size:24rpx;
}