<view id="wrap">
  <view class='inner'>
    <!-- TITLE START -->
    <!-- <view class='title'>个人中心</view> -->
    <view class='content-list'>
      <view class='c-l-info'>
          <image src='../../static/images/public/big_portrait.png'class="c-logo" />
          <view class='c-info-text'>
            <text class='c-info-title'>周翔</text>
            <view class='c-msg m-b10'>个人用户</view>
            <view class='c-msg m-b10'>企业编号：007</view>
          </view>
          <!-- company info start -->
      </view>
      <!-- USER OPTION START -->
      <view class='user-option'>
          <view class='u-o-list m-b50'>
            <text>编辑公司信息</text>
            <image class='u-o-icon1' src='../../static/images/public/edit.png'/>
          </view>
          <view class='u-o-list m-b50'>
            <text>我的证书</text>
            <image class='u-o-icon1' src='../../static/images/public/edit.png'/>
          </view>
          <navigator url="privacySettings" class='u-o-list m-b50'>
            <text>隐私设置</text>
            <image class='u-o-icon1' src='../../static/images/public/edit.png'/>
          </navigator>
          <navigator url="setting" class='u-o-list m-b50'>
            <text>设置</text>
            <image class='u-o-icon1' src='../../static/images/public/edit.png'/>
          </navigator>
          <navigator url="myPurse" class='u-o-list m-b50'>
            <text>我的钱包</text>
            <image class='u-o-icon2' src='../../static/images/public/right_arrow.png'/>
          </navigator>
          <view class='u-o-list m-b50'>
            <text>实名认证</text>
            <text class='u-o-text'>已认证</text>
          </view>
          <navigator url="help" class='u-o-list m-b50'>
            <text>帮助与反馈</text>
            <image class='u-o-icon2' src='../../static/images/public/right_arrow.png'/>
          </navigator>
          <navigator url="help" class='u-o-list m-b50'>
            <text>我的收藏</text>
            <image class='u-o-icon2' src='../../static/images/public/right_arrow.png'/>
          </navigator>
      </view>
    </view>
  </view>
</view>
<!-- FOOTER START -->
<view class='footer-height'></view>
