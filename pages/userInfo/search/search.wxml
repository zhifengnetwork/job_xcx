<!--pages/userInfo/search.wxml-->
<view class='search-wrap'>

  <view class='g-header-'>
    <view class='-header-cont'>
      <view class='search-cont'>
        <input type='text' class='search'  placeholder="输入所要查找的内容" bindblur='selecKeyWord' />
        <image class="search-icon" src='../../../static/images/public/search_icon.png'></image>
      </view>
      <!-- <text class='login_tab' bindtap='searchInfp'>搜索</text> -->
    </view>
    <view class='subtitle overH search-subtitle'>
        <text bindtap='changStatus' class="{{cStatus==0?'stss-active':''}}" data-status="0">企业</text>
        <text bindtap='changStatus' class="{{cStatus==1?'stss-active':''}}" data-status="1">服务商</text>
    </view>
  </view>

  <view class='inner inner-cont'>
      <view class='wx-no-info' wx:if="{{isShowInfo}}">
            <!-- <image class="wx-no-icon" src='../../../static/images/public/none.png'/> -->
            <view>暂无数据</view>
      </view>
      <view wx:if="{{cStatus==0}}">
          <!-- <navigator url="../../postionDetail/postionDetail?id={{item.id}}" class='c-l-info' wx:for="{{list.recruit}}">
              <image src='{{item.logo}}'class="c-logo" />
              <view class='c-info-text'>
                <text class='c-info-title'>{{item.title}}</text>
                <view class='c-info-msg'>
                  <text class='c-msg'>{{item.salary}}K</text>
                  <text class='c-msg2'>{{item.work_age}} 年</text>
                </view>
              </view>
              <view class='company-info'>
                  <text>企业</text>
                  <view class='company-status'>
                      <view class='company-status'>{{item.regtype == 0 ? "无需证书":"需要证书"}}</view>
                  </view>
              </view>
          </navigator> -->
          <navigator url="../../postionDetail/postionDetail?id={{item.id}}" wx:for="{{list.recruit}}">
              <view class='global-search-wrap'>
                <view class='search-job-list'>
                  <view class='c-info-title global-search-cont'>
                      <text>{{item.title}}</text>
                      <text class="user-color-">{{item.salary=='面议' ? item.salary : item.salary+'/月'}}</text>
                  </view>
                  <view class='s-portrait global-search-cont'>
                      <view class="">
                          <image src='{{item.logo}}' class='portrait-icon'/>
                          <text>{{item.company_name}}</text>
                      </view>
                      <view class='company-status fr'>{{item.regtype == 0 ? "无需证书":"需要证书"}}</view>
                  </view>
                  <view class='c-info-msg global-search-cont'>
                        <view>
                          <text class='c-msg2 '>{{item.city_str}}{{item.district_str}}</text>
                          <text class='c-msg2 '>{{item.work_age}}年 </text>
                          <!-- <text class='c-msg2 '>{{item.school_type}}</text> -->
                        </view>
                        <text class="posen-msg user-color-">企业</text>
                  </view>
                </view>
              </view>
          </navigator>

      </view>

      <view wx:if="{{cStatus==1}}">

          <navigator url="../../postionDetail/postionDetail?id={{item.id}}" wx:for="{{list.person}}">
              <view class='global-search-wrap'>
                <view class='search-job-list'>
                  <view class='c-info-title global-search-cont'>
                      <text>{{item.title}}</text>
                      <text class="user-color-">{{item.salary=='面议' ? item.salary : item.salary+'/月'}}</text>
                  </view>
                  <view class='s-portrait global-search-cont'>
                      <view class="">
                          <image src='{{item.logo}}' class='portrait-icon'/>
                          <text>{{item.company_name}}</text>
                      </view>
                      <view class='company-status fr'>{{item.regtype == 0 ? "无需证书":"需要证书"}}</view>
                  </view>
                  <view class='c-info-msg global-search-cont'>
                        <view>
                          <text class='c-msg2 '>{{item.city_str}}{{item.district_str}}</text>
                          <text class='c-msg2 '>{{item.work_age}}年 </text>
                          <!-- <text class='c-msg2 '>{{item.school_type}}</text> -->
                        </view>
                        <text class="posen-msg user-color-">服务商</text>
                  </view>
                </view>
              </view>
          </navigator>
          <!-- <navigator url="../../postionDetail/postionDetail?id={{item.id}}" class='c-l-info' wx:for="{{list.person}}">
              <image src='{{item.logo}}'class="c-logo" />
              <view class='c-info-text'>
                <text class='c-info-title'>{{item.title}}</text>
                <view class='c-info-msg'>
                  <text class='c-msg'>{{item.salary}}K</text>
                  <text class='c-msg2'>{{item.work_age}} 年</text>
                </view>
              </view>
              <view class='company-info'>
                  <text>服务商</text>
                  <view class='company-status'>
                      <view class='company-status'>{{item.regtype == 0 ? "无需证书":"需要证书"}}</view>
                  </view>
              </view>
          </navigator> -->
      </view>
  </view>

</view>
