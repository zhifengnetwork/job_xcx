<view class="inner">
  <!-- 上传身份证正面 -->
  <view class='idPic'>
    <view class='-pic-subtitle'>上传职业证书</view>
    <view>
      <view class='id-card-top'>
        <image src='{{positiveImg}}'></image>
        <image class='addPic' bindtap='addPosPic' src='../../static/images/public/plus.png'></image>
      </view>
      <input class="addTextBox" placeholder='请输入职业证书名称'/>
      <!-- <text>点击上传证照</text> -->
    </view>
    <view class='file-inner'>
      <button class='file-btn'></button> 
      <image class='f-plus-' src='../../static/images/public/plus.png'/>
    </view>
  </view>
  <view class='a-more-pic'>添加一张</view>
  <view class='height'></view>
  <button class='set-option' bindtap="formSubmit">提交</button>
</view>
