
  <!-- HEADER START -->
  <view class='g-header-'>
    <view class='-header-cont'>
      <navigator url='../../search/pSearch' class='search-cont s-nosearch-wrap'>
        <view class='search search-nosearch third-bColor-1'>输入所要查找的内容</view>
        <image class="search-icon" src='../../../static/images/public/search_icon.png'></image>
      </navigator>
    </view>
    <view class='subtitle overH'>
        <picker value="{{cat_id}}" range="{{jobArray}}" bindchange="jobChange" range-key="cat_name" range-value="cat_id">
          <view class="picker subtitle-msg">
            {{jobArray[jobIndex].cat_name}} 
          </view>
        </picker>
    </view>
  </view>
  <view class='header-heigh'></view>
  <!-- HEADER END -->
  <view class='inner'>
    <navigator url="../personalDetail/personalDetail?id={{item.id}}" class='c-l-info' wx:for="{{hiringData}}" wx:key="key">
        <view class='search-job-list'>
          <view class='c-info-title'>
              <text wx:if="{{item.cat_name!=null}}">{{item.cat_name}}</text>
              <text wx:else></text>
              
          </view>
          <view class='c-info-msg overH'>
            <text class='c-msg2 fl'>{{item.work_age}}年</text>
          </view>
          <view class='s-portrait'>
            <image src='{{item.avatar}}' class='portrait-icon'/>
            <text>{{item.name}}</text>
          </view>
          <view class='overH'></view>
        </view>
        <view class='company-status third-bg- fr'>
            <text wx:if="{{item.images==1}}">有证书</text>
            <text wx:else>无证书</text>
        </view>
    </navigator>
    <!-- <view class='more-recommond'>更多推荐</view> -->
  </view>
  <!-- FOOTER TARBAR START -->
  <view class='footer-height'></view>
  <import src="../../template/tabbar.wxml" />
  <template is="tabBar" data="{{tabBar}}" />
