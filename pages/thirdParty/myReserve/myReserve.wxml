<!--pages/company/uCollect.wxml-->
<view class='inner'>
  <view class='c-l-info'>
    <scroll-view class="list" scroll-y="true" style="height:{{scrollHeight}}px;" bindscrolltolower="scrollLower">
      <navigator url="../postionDetail/postionDetail?id={{item.person_id}}" class='search-job-list' wx:for="{{bookListData}}" wx:key="key">
        <view class='c-info-title'>{{item.job_type}}</view>
        <view class='c-info-msg overH'>
          <!-- <text class='c-msg fl'>广州市白云区</text> -->
          <text class='c-msg2 fl'>{{item.work_age}}年</text>
          <view class='company-status company-bg- fr' wx:if="{{item.images == 0}}">无证书</view>
          <view class='company-status company-bg- fr' wx:if="{{item.images == 1}}">有证书</view>
        </view>
        <!-- <view class='c-msg3'>会计证书</view> -->
        <view class='s-portrait'>
          <image src='{{item.avatar}}' class='portrait-icon'/>
          <text>{{item.name}}</text>
        </view>
        <view class='overH'></view>
      </navigator>
    </scroll-view>  
  </view>
  <view class="loading-complete" hidden="{{!searchLoadingComplete}}">我有底线哦...</view> 
  <view class='wx-no-info' wx:if="{{isShowR}}">
      <image class='wx-no-icon' src='../../../static/images/public/none.png'></image>
      <view>暂无数据</view>
  </view>
</view>